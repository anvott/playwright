{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,//fix issue json type
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist"
    },
    "include": ["**/*.ts", "tests", "page"],
    "exclude": ["node_modules", "dist"]
}